    "css-loader": "^1.0.0",
    "express": "~3.2.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "mongodb": "~3.0.8",
    "nodemon": "^1.18.2",
    "react": "^16.4.1",
